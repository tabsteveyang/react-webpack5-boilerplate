{
  "name": "react-webpack5-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for starting multiple projects with React, Redux controlled by a root project.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-project1": "cd projects/project1 && npm run dev",
    "dev-project2": "cd projects/project2 && npm run dev",
    "prod-project1": "cd projects/project1 && npm run prod",
    "prod-project2": "cd projects/project2 && npm run prod",
    "prod": "npm-run-all prod-*"
  },
  "author": "",
  "license": "",
  "workspaces": [
    "projects/*"
  ],
